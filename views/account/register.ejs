<!-- title -->
 <% if (title) { %>
<h1><%= title %></h1>
<% } else{
    res.redirect('/')
} %>

<%- messages() %>
<div class="register">
    <form action="/account/register" method="post">
        <p>First Name *<input type="text" name="account_firstname" required></p>
        <p>Last Name *<input type="text" name="account_lastname" required></p>
        <p>Email *<input type="email" name="account_email" required></p>
        <p>Password *<input type="password" name="account_password" placeholder="Must be 12 char,1 cap,1 num,1 special." pattern=".{12,}" required></p>
        <input type="submit" name="Register">
    </form>
</div>