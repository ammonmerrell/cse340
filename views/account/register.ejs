<!-- title -->
 <% if (title) { %>
<h1><%= title %></h1>
<% } else{
    res.redirect('/')
} %>

<%- messages() %>
<div class="register">
    <form>
        <p>First Name *<input type="text" name="account_firstname" required></p>
        <p>Last Name *<input type="text" name="account_lastname" required></p>
        <p>Email *<input type="email" name="account_email" required></p>
        <p>Password<input type="password" name="account_password" required></p>
        <input type="submit" name="Register">
    </form>
</div>